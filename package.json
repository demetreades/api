{
	"name": "backend",
	"version": "1.0.0",
	"main": "src/server.js",
	"type": "module",
	"scripts": {
		"start": "node src/server.js",
		"dev": "nodemon",
		"test": "mocha './src/*/**.spec.js'",
		"coverage": "nyc npm run test",
		"up": "docker compose up",
		"up:rebuild": "docker compose build --no-cache && npm run up",
		"down": "docker compose down",
		"down:clean": "docker compose down --rmi all",
		"release": "release-it",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"prettier": "prettier --write .",
		"kill": "kill -9 $(lsof -t -i:5555)",
		"permissions": "chmod -R 777 ./node_modules",
		"prepare:husky": "npx husky-init && npm i && npx husky add .husky/commit-msg 'npx --no -- commitlint --edit \"$1\"'"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"description": "",
	"dependencies": {
		"@elastic/ecs-pino-format": "1.3.0",
		"compression": "1.7.4",
		"cookie-parser": "1.4.6",
		"cors": "2.8.5",
		"express": "4.18.2",
		"express-session": "1.17.3",
		"helmet": "7.0.0",
		"mongodb": "5.6.0",
		"pino": "8.14.1",
		"pino-elasticsearch": "6.4.0"
	},
	"devDependencies": {
		"@commitlint/cli": "17.6.5",
		"@commitlint/config-conventional": "17.6.5",
		"@release-it/conventional-changelog": "^5.1.1",
		"chai": "4.3.7",
		"dotenv": "16.0.3",
		"eslint": "8.39.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-jsdoc": "46.1.0",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-promise": "6.1.1",
		"eslint-plugin-security": "1.7.1",
		"husky": "8.0.0",
		"jsdoc": "4.0.2",
		"jsdoc-to-markdown": "8.0.0",
		"mocha": "10.2.0",
		"nyc": "15.1.0",
		"pino-pretty": "10.0.0",
		"prettier-eslint": "15.0.1",
		"prettier-plugin-jsdoc": "0.4.2",
		"release-it": "^15.10.5",
		"sinon": "15.1.0",
		"supertest": "6.3.3"
	},
	"lint-staged": {
		"**/*.js": [
			"npm run lint",
			"npm run prettier"
		],
		"**/*.spec.js": [
			"npm test"
		]
	},
	"nyc": {
		"include": [
			"**/*.js"
		],
		"exclude": [
			"**/*.spec.js",
			"node_modules",
			"coverage"
		],
		"reporter": [
			"lcov",
			"html",
			"text-summary",
			"text"
		],
		"all": true,
		"check-coverage": true,
		"lines": 85
	},
	"release-it": {
		"git": {
			"commitMessage": "chore: release v${version}"
		},
		"github": {
			"release": true
		},
		"npm": {
			"publish": false
		},
		"plugins": {
			"@release-it/conventional-changelog": {
				"infile": "CHANGELOG.md",
				"preset": {
					"name": "conventionalcommits",
					"types": [
						{
							"type": "feat",
							"section": "Features"
						},
						{
							"type": "fix",
							"section": "Bug Fixies"
						}
					]
				}
			}
		}
	},
	"engines": {
		"node": "18.16.0"
	}
}